<launch>
<include file="$(find riptide_hardware)/launch/cameras.launch" />

  <!-- Config and weights folder -->
  <arg name="yolo_weights_path"          default="$(env HOME)/osu-uwrt/riptide_software/yolo_weights"/>
  <arg name="yolo_config_path"           default="$(find riptide_hardware)/yolo_network_config"/>

  <!-- Load parameters -->
  <rosparam command="load" ns="darknet_ros" file="$(find darknet_ros)/config/ros.yaml"/>
  <rosparam command="load" ns="darknet_ros" file="$(find riptide_hardware)/yolo_network_config/yolov3-tiny_RS2018.yaml"/>

  <!-- Start darknet and ros wrapper -->
  <node pkg="darknet_ros" type="darknet_ros" name="darknet_ros" output="screen" launch-prefix="">
    <param name="weights_path"          value="$(arg yolo_weights_path)" />
    <param name="config_path"           value="$(arg yolo_config_path)" />
  </node>

 <!--<node name="republish" type="republish" pkg="image_transport" output="screen" 	args="compressed in:=/front_camera/image_raw raw out:=/camera/image_raw" /> -->
</launch>
